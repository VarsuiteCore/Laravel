{
    "name": "varsuite-core/laravel",
    "description": "Varsuite Core integration for Laravel",
    "keywords": [
        "varsuite",
        "core",
        "laravel"
    ],
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "VarsuiteCore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VarsuiteCore\\Tests\\": "tests/"
        }
    },
    "homepage": "https://vs-core.com",
    "authors": [
        {
            "name": "James King",
            "email": "james.king@vs-core.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-zip": "*",
        "doctrine/dbal": "*",
        "illuminate/contracts": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "spatie/db-dumper": "^3.8"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "VarsuiteCore\\VarsuiteCoreServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9 || ^10"
    }
}
